<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SMA\SMABundle\Entity\Nilai" table="nilai">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nis" type="string" column="nis" length="255" nullable="false"/>
    <field name="tugas" type="string" column="tugas" length="255" nullable="false"/>
    <field name="uts" type="string" column="uts" length="255" nullable="false"/>
    <field name="uas" type="string" column="uas" length="200" nullable="false"/>
    <field name="matapelajaran" type="string" column="matapelajaran" length="200" nullable="false"/>
    <field name="nama" type="string" column="nama" length="200" nullable="false"/>
  </entity>
</doctrine-mapping>
