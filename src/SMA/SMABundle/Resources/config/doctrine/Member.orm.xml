<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SMA\SMABundle\Entity\Member" table="member">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="20" nullable="false"/>
    <field name="password" type="string" column="password" length="20" nullable="false"/>
    <field name="status" type="string" column="status" length="20" nullable="true"/>
    <field name="nama" type="string" column="nama" length="200" nullable="true"/>
    <field name="alamat" type="string" column="alamat" length="400" nullable="true"/>
    <field name="email" type="string" column="email" length="40" nullable="true"/>
  </entity>
</doctrine-mapping>
