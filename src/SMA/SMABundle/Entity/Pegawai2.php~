<?php

namespace SMA\SMABundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Pegawai2
 */
class Pegawai2
{
    /**
     * @var string
     */
    private $nis;

    /**
     * @var string
     */
    private $nama;

    /**
     * @var string
     */
    private $jabatan;

    /**
     * @var string
     */
    private $bidang;

    /**
     * @var string
     */
    private $telp;

    /**
     * @var string
     */
    private $email;

    /**
     * @var string
     */
    private $alamat;

    /**
     * @var \DateTime
     */
    private $tglLahir;

    /**
     * @var string
     */
    private $agama;

    /**
     * @var string
     */
    private $jk;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set nis
     *
     * @param string $nis
     * @return Pegawai2
     */
    public function setNis($nis)
    {
        $this->nis = $nis;

        return $this;
    }

    /**
     * Get nis
     *
     * @return string 
     */
    public function getNis()
    {
        return $this->nis;
    }

    /**
     * Set nama
     *
     * @param string $nama
     * @return Pegawai2
     */
    public function setNama($nama)
    {
        $this->nama = $nama;

        return $this;
    }

    /**
     * Get nama
     *
     * @return string 
     */
    public function getNama()
    {
        return $this->nama;
    }

    /**
     * Set jabatan
     *
     * @param string $jabatan
     * @return Pegawai2
     */
    public function setJabatan($jabatan)
    {
        $this->jabatan = $jabatan;

        return $this;
    }

    /**
     * Get jabatan
     *
     * @return string 
     */
    public function getJabatan()
    {
        return $this->jabatan;
    }

    /**
     * Set bidang
     *
     * @param string $bidang
     * @return Pegawai2
     */
    public function setBidang($bidang)
    {
        $this->bidang = $bidang;

        return $this;
    }

    /**
     * Get bidang
     *
     * @return string 
     */
    public function getBidang()
    {
        return $this->bidang;
    }

    /**
     * Set telp
     *
     * @param string $telp
     * @return Pegawai2
     */
    public function setTelp($telp)
    {
        $this->telp = $telp;

        return $this;
    }

    /**
     * Get telp
     *
     * @return string 
     */
    public function getTelp()
    {
        return $this->telp;
    }

    /**
     * Set email
     *
     * @param string $email
     * @return Pegawai2
     */
    public function setEmail($email)
    {
        $this->email = $email;

        return $this;
    }

    /**
     * Get email
     *
     * @return string 
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set alamat
     *
     * @param string $alamat
     * @return Pegawai2
     */
    public function setAlamat($alamat)
    {
        $this->alamat = $alamat;

        return $this;
    }

    /**
     * Get alamat
     *
     * @return string 
     */
    public function getAlamat()
    {
        return $this->alamat;
    }

    /**
     * Set tglLahir
     *
     * @param \DateTime $tglLahir
     * @return Pegawai2
     */
    public function setTglLahir($tglLahir)
    {
        $this->tglLahir = $tglLahir;

        return $this;
    }

    /**
     * Get tglLahir
     *
     * @return \DateTime 
     */
    public function getTglLahir()
    {
        return $this->tglLahir;
    }

    /**
     * Set agama
     *
     * @param string $agama
     * @return Pegawai2
     */
    public function setAgama($agama)
    {
        $this->agama = $agama;

        return $this;
    }

    /**
     * Get agama
     *
     * @return string 
     */
    public function getAgama()
    {
        return $this->agama;
    }

    /**
     * Set jk
     *
     * @param string $jk
     * @return Pegawai2
     */
    public function setJk($jk)
    {
        $this->jk = $jk;

        return $this;
    }

    /**
     * Get jk
     *
     * @return string 
     */
    public function getJk()
    {
        return $this->jk;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
